generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Leaderboard {
  id    Int    @id @default(autoincrement())
  user  String 
  time  Float    
  createdAt   DateTime  @default(now())
}

model Character {
  id           Int     @id @default(autoincrement())
  name         String  @unique 
  x            Int    
  y            Int     
}

model Session {
  id        String   @id
  sid       String   @unique
  data      String
  expiresAt   DateTime
}