generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Leaderboard {
  id    Int    @id @default(autoincrement())
  user  String 
  time  Int    
}

model Game {
  id           Int     @id @default(autoincrement())
  character    String  
  x            Int    
  y            Int     
}
